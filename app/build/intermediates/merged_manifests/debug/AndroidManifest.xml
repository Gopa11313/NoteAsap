<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.noteasap"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.NoteAsap" >
        <activity android:name="com.example.noteasap.Pupup.EditProfileActivity" />
        <activity
            android:name="com.example.noteasap.ContentActivity"
            android:label="@string/title_activity_bookmark_content"
            android:theme="@style/Theme.NoteAsap.NoActionBar" />
        <activity android:name="com.example.noteasap.UploadNotesActivity" />
        <activity android:name="com.example.noteasap.SignUpActivity" />
        <activity android:name="com.example.noteasap.LoginActivity" />
        <activity android:name="com.example.noteasap.Third_Activity" />
        <activity android:name="com.example.noteasap.SecondActivity" />
        <activity android:name="com.example.noteasap.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>